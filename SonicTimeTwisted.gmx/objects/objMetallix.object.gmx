    x-=2;
    if instance_exists(objLevel.player[0]) {
        if x &lt; objLevel.player[0].camera.left + 64{
            state = 5;
        }
    }
break;
case 4: // move w/ laser right
    x+=2;
    
     screen_shake();
     if !audio_is_playing(sndQuake)
