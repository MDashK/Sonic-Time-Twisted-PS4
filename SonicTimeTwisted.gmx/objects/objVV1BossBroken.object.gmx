case 2: // Turn
    sprite_index= sprMetalIdle;;
    image_speed = .5;
    image_index=0;
    state = 3;

break;
case 3: // Init Laugh
    if image_index == 2 {
    
        image_speed = 0;
        state=4;
        alarm[1] = 120;
        play_sfx(sndMetalLaugh,1,true);
    }
    
break;
case 4: //  Laughing
    y += cos(offset)*0.5;
    offset += 0.25;
    if alarm[1]==0 {
        state=5;
        play_sfx(sndExplosionGeneral,2);
    }
    
break;
case 5: // flash intro
    y += cos(offset)*0.5;
    offset += 0.25;
    
